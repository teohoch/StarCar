<p id="notice"><%= notice %></p>

<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>License plate</th>
      <th>Year</th>
      <th>Color</th>
      <th>Milage</th>
      <th>Maintenances</th>
      <th>Fueltype</th>
      <th>Transmission</th>
      <th>Prepaid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.brand %></td>
        <td><%= car.model %></td>
        <td><%= car.license_plate %></td>
        <td><%= car.year %></td>
        <td><%= car.color %></td>
        <td><%= car.milage %></td>
        <td><%= car.maintenances %></td>
        <td><%= car.Fuel %></td>
        <td><%= car.Transmission %></td>
        <td><%= car.prepaid %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
